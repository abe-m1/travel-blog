<script type="text/javascript">

var locations = [
  ['place 1', 51.50, 0.000, 'museum'],
  ['place 2', 51.40, 0.000, 'castle'],
  ['place 3', 51.30, 0.000, 'park'],
  ['place 4', 51.60, 0.000, 'pub'],
